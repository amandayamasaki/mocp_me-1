{
  "name": "mocp.me",
  "version": "2.0.0",
  "description": "React based application to explore the works of the Museum of Contemporary Photography",
  "main": "index.js",
  "scripts": {
    "server": "node-dev server.js",
    "client": "node startup-scripts/start-client.js",
    "build": "node startup-scripts/build.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "concurrently \"node server.js\" \"npm run client\"",
    "heroku-postbuild": "cd client && npm install --only=dev && npm install && npm run build"
  },
  "devDependencies": {
    "concurrently": "^3.5.0",
    "node-dev": "^3.1.3"
  },
  "repository": "https://github.com/mocp-me/mocp_me",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "fs": "^0.0.1-security",
    "mysql2": "^1.5.1",
    "path": "^0.12.7",
    "react-router": "^4.2.0",
    "sequelize": "^4.32.2"
  }
}
